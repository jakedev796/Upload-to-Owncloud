{
  "manifest_version": 2,
  "name": "Upload to Owncloud",
  "version": "1.0",
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "notifications",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "owncloud-uploader@example.com",
      "strict_min_version": "68.0"
    }
  }
}